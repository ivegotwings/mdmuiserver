{
  "entrypoint": "src/views/index.hjs",
  "shell": "src/elements/app-main/main-app.html",
  "fragments": [
    "src/elements/liquid-entity-data-get/demo/index.html"
  ],
  "sources": [
   "src/**/*",
   "images/**/*",
   "manifest.json",
   "app.js",
   "bower.json",
   "sampledata/**/*",
   "offline-data/**/*",
   "tenant-configs/**/*",
   "logs/empty.json",
   ".babelrc",
   "service-worker.js",
   "index.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/webcomponents-lite.min.js",
    "bower_components/web-component-tester/**/*",
    "bower_components/accessibility-developer-tools/**/*",
    "bower_components/async/**/*",
    "bower_components/chai/**/*",
    "bower_components/lodash/**/*",
    "bower_components/mocha/**/*",
    "bower_components/sinon-chai/**/*",
    "bower_components/sinonjs/**/*",
    "bower_components/stacky/**/*",
    "bower_components/test-fixture/**/*"
  ],
  "lint": {
    "rules": ["polymer-1"],
     "ignoreWarnings": []
  },
  "builds": [{
    "name": "unbundled",
    "bundle": false,
    "js": {"minify": false, "compile": true},
    "css": {"minify": false},
    "html": {"minify": false},
    "insertPrefetchLinks": true,
    "addServiceWorker": true,
    "addPushManifest": false
  }]
}