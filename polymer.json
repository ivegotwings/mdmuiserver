{
  "serverSources": [
    "app.js",
    "index.html",
    "dist/src/server/**/*",
    "dist/src/shared/**/*",
    "dist/src/views/**/*",
    "dist/src/resources/**/*",
    "logs/**/*",
    "offline-data/**/*",
    "service-worker.js",
    "ecosystem.config.js",
    "package.json"
  ],
  "lint": {
    "rules": ["polymer-3"],
    "ignoreWarnings": [
      "set-unknown-attribute",
      "databind-with-unknown-property"
    ]
  }
}