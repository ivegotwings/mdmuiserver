{
  "entrypoint": "index.html",
  "shell": "src/elements/app-main/main-app.html",
  "fragments": [
    "src/elements/app-dashboard/app-dashboard.html",
    "src/elements/app-business-function/app-business-function.html",
    "src/elements/app-entity-discovery/app-entity-discovery.html",
    "src/elements/app-reference-discovery/app-reference-discovery.html",
    "src/elements/app-entity-manage/app-entity-manage.html",
    "src/elements/app-assets-discovery/app-assets-discovery.html",
    "src/elements/app-task-detail/app-task-detail.html",
    "src/elements/app-system-healthcheck/app-system-healthcheck.html",
  ],
  "sources": [
    "src/elements/**/*",
    "src/images/**/*",
    "src/resources/**/*",
    "src/scripts/**/*",
    "src/shared/**/*",
    "service-worker.js",
    "favicon.ico"
  ],
  "serverSources": [
    "app.js",
    "index.html",
    "src/server/**/*",
    "src/shared/**/*",
    "src/views/**/*",
    "src/resources/**/*",
    "logs/**/*",
    "offline-data/**/*",
    "service-worker.js",
    "ecosystem.config.js",
    "package.json",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*",
    "bower_components/polymer/**/*",
    "bower_components/web-animations-js/**/*",
    "!bower_components/web-component-tester/**/*",
    "bower_components/accessibility-developer-tools/**/*",
    "bower_components/async/**/*",
    "bower_components/chai/**/*",
    "bower_components/lodash/**/*",
    "bower_components/mocha/**/*",
    "bower_components/sinon-chai/**/*",
    "bower_components/sinonjs/**/*",
    "bower_components/stacky/**/*",
    "bower_components/test-fixture/**/*",
    "bower_components/**/*.map",
    "bower_components/**/*.css",
    "bower_components/resize-observer-polyfill/**/*",
    "bower_components/pdf-viewer/**/*",
    "bower_components/pdfjs-dist/**/*",
    "bower_components/paper-spinner/**/*",
    "bower_components/paper-styles/**/*",
    "bower_components/font-roboto/**/*"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ],
    "ignoreWarnings": [
      "multiple-doc-comments",
      "unknown-polymer-behavior",
      "undefined-elements",
      "set-unknown-attribute",
      "databind-with-unknown-property",
      "style-into-template",
      "databinding-calls-must-be-functions"
    ]
  }
}
