{
  "entrypoint": "src/elements/app-main/main-app.html",
  "shell": "",
  "fragments": [],
  "sources": [
    "images/**/*",
    "sampledata/**/*",
    "offline-data/**/*",
    "tenant-configs/**/*",
    "bower.json",
    "app.js",
    "logs/empty.json",
    ".babelrc",
    "service-worker.js",
    "src/data/**/*",
    "src/images/**/*",
    "src/resources/**/*",
    "src/scripts/**/*",
    "src/server/**/*",
    "src/shared/**/*",
    "src/views/**/*",
    "src/elements/app-main/main-app.js",
    "src/scripts/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/web-component-tester/**/*",
    "bower_components/accessibility-developer-tools/**/*",
    "bower_components/async/**/*",
    "bower_components/chai/**/*",
    "bower_components/lodash/**/*",
    "bower_components/mocha/**/*",
    "bower_components/sinon-chai/**/*",
    "bower_components/sinonjs/**/*",
    "bower_components/stacky/**/*",
    "bower_components/test-fixture/**/*"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ],
    "ignoreWarnings": [
      "multiple-doc-comments",
      "unknown-polymer-behavior",
      "undefined-elements",
      "set-unknown-attribute",
      "databind-with-unknown-property",
      "style-into-template",
      "databinding-calls-must-be-functions"
    ]
  },
  "builds": [
    {
      "preset": "es6-unbundled",
      "js": {
        "minify": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      }
    }
  ]
}